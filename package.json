{
  "name": "wix-sso",
  "version": "1.0.0",
  "description": "Code to be shared across sesam Wix sites",
  "homepage": "https://sesam.io",
  "scripts": {
    "start": "parcel serve index.html --port 8001 --open",
    "build": "parcel build --no-cache --no-content-hash",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "sso-flow-dev": "artifact/sso-flow.dev.js",
  "sso-flow-prod": "artifact/sso-flow.min.js",
  "ulp-prod": "artifact/ulp.js",
  "targets": {
    "sso-flow-dev": {
      "source": "src/packages/ssoFlow/index.ts",
      "distDir": "./artifact",
      "optimize": false,
      "sourceMap": {
        "inline": true
      },
      "context": "browser",
      "outputFormat": "global",
      "scopeHoist": false,
      "engines": {
        "browsers": [
          "last 5 Chrome versions"
        ]
      }
    },
    "sso-flow-prod": {
      "source": "src/packages/ssoFlow/index.ts",
      "distDir": "./artifact",
      "optimize": true,
      "sourceMap": false,
      "context": "browser",
      "outputFormat": "global",
      "engines": {
        "browsers": [
          "last 2 versions, not dead, > 0.2%"
        ]
      }
    },
    "ulp-prod": {
      "source": "src/packages/universalLoginPage/index.ts",
      "distDir": "./artifact",
      "optimize": false,
      "sourceMap": false,
      "context": "browser",
      "outputFormat": "global",
      "scopeHoist": false,
      "engines": {
        "browsers": "defaults and fully supports es6-module"
      }
    }
  },
  "author": "Sesam.io",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@auth0/auth0-spa-js": "^2.1.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "parcel": "^2.10.0",
    "typescript": "4.4.4",
    "yarn": "^1.22.19"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "typescript",
    "javascript",
    "sesam",
    "wix"
  ],
  "repository": "git@github.com:sesam-io/wix-sso.git"
}
