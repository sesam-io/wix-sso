{
  "name": "wix-sso",
  "version": "1.0.0",
  "description": "Code to be shared across sesam Wix sites",
  "homepage": "https://sesam.io",
  "scripts": {
    "start": "parcel serve index.html --port 8001 --open",
    "build": "parcel build"
  },
  "dev": "artifact/sso-flow.js",
  "prod": "artifact/sso-flow.min.js",
  "targets": {
    "dev": {
      "source": "src/index.ts",
      "distDir": "./artifact",
      "optimize": false,
      "sourceMap": true,
      "context": "browser"
    },
    "prod": {
      "source": "src/index.ts",
      "distDir": "./artifact",
      "optimize": true,
      "context": "browser"
    }
  },
  "author": "Sesam.io",
  "license": "ISC",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.2"
  },
  "devDependencies": {
    "parcel": "^2.10.0",
    "typescript": "4.4.4",
    "yarn": "^1.22.19"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "typescript",
    "javascript",
    "sesam",
    "wix"
  ],
  "repository": "git@github.com:sesam-io/wix-sso.git"
}
